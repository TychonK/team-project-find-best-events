{{#each this}}

<div class="modal__image--little">
    <img class="modal__image--little" src="{{images.2.url}}" alt="{{name}}" />
</div>

<div class="modal-content__container ">
    <div class="modal-content__item">
        <div class="modal__image--big">
            <img class="modal__image" src="{{images.2.url}}" alt="{{name}}" />
        </div>

    </div>

    <div class="modal-content__item">
        <ul class="modal-info__list ">
            <h3 class="modal-info__topic">INFO</h3>
            <p class="modal-info__text info__text">
                {{#if info}}
                {{info}}
                {{else}}
                There is no information about this event
                {{/if}}</p>
            </li>
            <li class="modal-info__item">
                <h3 class="modal-info__topic">WHO</h3>
                <p>{{name}}</p>
            </li>
            <li class="modal-info__item">
                <h3 class="modal-info__topic">WHEN</h3>
                <p class="modal-info__text text-margin">{{dates.start.localDate}}</p>
                <p class="modal-info__text">{{dates.start.localTime}} ({{dates.timezone}})</p>
            </li>
            <li class="modal-info__item">
                <h3 class="modal-info__topic">WHERE</h3>
                <p class="modal-info__text text-margin">{{dates.timezone}}</p>
                <p class="modal-info__text">{{_embedded.venues.0.name}}</p>
            </li>
            <li class="modal-info__item">
                <h3 class="modal-info__topic">PRICES</h3>

                {{#if priceRanges.0.type}}
                    <p class="price-tickets modal-info__text"><svg class="barcode-icon" width="29" height="21" viewBox="0 0 29 21" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path d="M3.22222 0.833496L0 0.833496L0 20.1668H3.22222L3.22222 0.833496Z" fill="#0E0E0E"/>
                    <path d="M11.3266 0.833496L8.10439 0.833496L8.10439 20.1668H11.3266L11.3266 0.833496Z" fill="#0E0E0E"/>
                    <path d="M16.2088 0.833496L12.9866 0.833496L12.9866 20.1668H16.2088L16.2088 0.833496Z" fill="#0E0E0E"/>
                    <path d="M28.9999 0.833496L24.2154 0.833496L24.2154 20.1668H28.9999L28.9999 0.833496Z" fill="#0E0E0E"/>
                    <path d="M6.44449 0.833496L4.88219 0.833496L4.88219 20.1668H6.44449L6.44449 0.833496Z" fill="#0E0E0E"/>
                    <path d="M19.3333 0.833496L17.771 0.833496L17.771 20.1668H19.3333L19.3333 0.833496Z" fill="#0E0E0E"/>
                    <path d="M22.5555 0.833496L20.9932 0.833496L20.9932 20.1668H22.5555L22.5555 0.833496Z" fill="#0E0E0E"/>
                    </svg>{{priceRanges.0.type}}
                    {{priceRanges.0.min}} - {{priceRanges.0.max}} {{priceRanges.0.currency}} </p>
                </p>

                {{#if products}}
                <button type="button" id="buy-tickets" class="modal-btn_tickets">BUY TICKETS</button>
                {{/if}}
                {{/if}}

                 {{#if priceRanges.1.type}}
                    <p class="price-tickets modal-info__text"><svg class="barcode-icon" width="29" height="21" viewBox="0 0 29 21" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path d="M3.22222 0.833496L0 0.833496L0 20.1668H3.22222L3.22222 0.833496Z" fill="#0E0E0E"/>
